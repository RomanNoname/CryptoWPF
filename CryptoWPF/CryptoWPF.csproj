    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Image\en.png" />
    <None Remove="Image\ua.png" />
  </ItemGroup>

  <ItemGroup>
