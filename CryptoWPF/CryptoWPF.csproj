    <Content Include="Image\ua.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\CryptoContracts\CryptoBLL.csproj" />
  </ItemGroup>

</Project>
